{
  "name": "gpter-ai",
  "version": "0.0.1",
  "main": "dist-electron/main/index.js",
  "description": "Spin up GPT assistants in 2 clicks",
  "author": "gpter-ai",
  "contributors": [
    "ssaprank <stanislau.saprankou@gmail.com>",
    "dmitrybabich <dmitrybabichmail@gmail.com"
  ],
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "update-prompts": "(cd .. && sh scripts/prompts/build-prompts.sh)",
    "clean": "rm -rf release && rm -rf dist && rm -rf dist-electron",
    "release": "npm run clean && npm run build && electron-builder",
    "watch": "tsc --watch",
    "preview": "vite preview",
    "style": "prettier -c ./src && eslint ./src",
    "style:fix": "prettier -cw ./src && eslint ./src --fix",
    "style:check": "npm run style:fix && npm run style",
    "prepare": "cd .. && husky install",
    "test": "jest"
  },
  "dependencies": {
    "@cloudscape-design/components": "^3.0.248",
    "@cloudscape-design/global-styles": "^1.0.8",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@types/lodash": "^4.14.194",
    "autosize": "^6.0.1",
    "dexie": "^3.2.3",
    "dexie-react-hooks": "^1.1.3",
    "electron-updater": "^5.3.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "highlight.js": "^11.8.0",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "react-markdown": "^8.0.7",
    "ua-parser-js": "^1.0.35",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cloudscape-design/jest-preset": "^2.0.8",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/autosize": "^4.0.1",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-jest": "^29.5.0",
    "electron": "^23.2.0",
    "electron-builder": "^24.2.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "openai": "^3.2.1",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.60.0",
    "typescript": "^5.0.4",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.13.14",
    "vite-plugin-eslint": "^1.8.1"
  },
  "engines": {
    "node": ">=18.15.0"
  }
}
