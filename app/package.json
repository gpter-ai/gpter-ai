{
  "name": "app",
  "version": "2.2.0",
  "main": "dist-electron/main/index.js",
  "description": "Electron Vite React boilerplate.",
  "author": "草鞋没号 <308487730@qq.com>",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.prod.json && vite build && electron-builder",
    "watch": "tsc --watch",
    "preview": "vite preview",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "style": "prettier -c ./src && eslint ./src",
    "style:fix": "prettier -cw ./src && eslint ./src --fix",
    "style:check": "npm run style:fix && npm run style",
    "prepare": "cd .. && husky install",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@cloudscape-design/components": "^3.0.248",
    "@cloudscape-design/global-styles": "^1.0.8",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@types/lodash": "^4.14.194",
    "dexie": "^3.2.3",
    "dexie-react-hooks": "^1.1.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cloudscape-design/jest-preset": "^2.0.8",
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-jest": "^29.5.0",
    "electron": "^23.2.0",
    "electron-builder": "^24.2.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "openai": "^3.2.1",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.60.0",
    "typescript": "^5.0.4",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.13.14",
    "vite-plugin-eslint": "^1.8.1"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  }
}
